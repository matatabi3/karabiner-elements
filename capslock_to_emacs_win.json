{
  "title": "Capslock to Emacs and windows key binds.",
  "rules": [
    {
      "description": "CapsLock to Control",
      "manipulators": [
        {
          "from": {
            "key_code": "caps_lock",
            "modifiers": {
              "optional": [
                  "any"
              ]
            }
          },
          "to": [
            {
                "key_code": "right_command",
                "modifiers": [
                    "right_control",
                    "right_option"
                ]
            }
          ],
          "to_if_alone": [
            {
                "key_code": "left_control"
            }
          ],
          "type": "basic"
        }
      ]
    },
